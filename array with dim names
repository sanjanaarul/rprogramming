# Create a vector of values
values <- 1:24  # 24 elements

# Define the dimensions: 3 rows, 4 columns, 2 matrices
dimensions <- c(3, 4, 2)

# Define names for each dimension
row_names <- c("Row1", "Row2", "Row3")
col_names <- c("Col1", "Col2", "Col3", "Col4")
matrix_names <- c("Matrix1", "Matrix2")

# Create the array
my_array <- array(values, dim = dimensions,
                  dimnames = list(row_names, col_names, matrix_names))

# Display the array
cat("Array with dimension names:\n")
print(my_array)
